{
  "name": "vscode-git-history",
  "displayName": "Git File History",
  "description": "View or diff against previous versions of the current file.",
  "icon": "resources/logo-128x128.png",
  "version": "1.0.0",
  "license": "MIT",
  "main": "out/src/extension.js",
  "publisher": "fabiospampinato",
  "activationEvents": [
    "onCommand:gitHistory.fileHistory",
    "onCommand:gitHistory.fileHistoryToSide",
    "onCommand:gitHistory.fileDiff"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Git File History - Configuration",
      "properties": {
        "gitHistory.details.author.enabled": {
          "type": "boolean",
          "description": "Show the name of the commit's author",
          "default": true
        },
        "gitHistory.details.date.enabled": {
          "type": "boolean",
          "description": "Show the date of the commit",
          "default": true
        },
        "gitHistory.details.date.format": {
          "type": "string",
          "description": "Format used for displaying the date",
          "default": "YYYY-MM-DD HH:mm"
        },
        "gitHistory.details.hash.enabled": {
          "type": "boolean",
          "description": "Show the hash of the commit",
          "default": false
        },
        "gitHistory.details.hash.length": {
          "type": "number",
          "description": "Show only this number of characters from the end of the hash",
          "default": 7
        }
      }
    },
    "commands": [
      {
        "command": "gitHistory.fileHistory",
        "title": "Git: File History"
      },
      {
        "command": "gitHistory.fileHistoryToSide",
        "title": "Git: File History to the Side"
      },
      {
        "command": "gitHistory.fileDiff",
        "title": "Git: File Diff"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "publish": "vsce publish && npm publish",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/vscode-git-history/issues"
  },
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/vscode-git-history.git"
  },
  "engines": {
    "vscode": "^1.15.0"
  },
  "keywords": [
    "vscode",
    "git",
    "file",
    "history",
    "diff",
    "extension"
  ],
  "categories": [
    "Other"
  ],
  "dependencies": {
    "absolute": "0.0.1",
    "find-up": "^2.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.21.0",
    "pify": "^3.0.0",
    "simple-git": "^1.92.0",
    "temp": "^0.8.3"
  },
  "devDependencies": {
    "@types/node": "^6.0.40",
    "tslint": "^4.4.2",
    "typescript": "~2.2.0",
    "vscode": "^1.1.4"
  }
}
